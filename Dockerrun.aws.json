{
    "AWSEBDockerrunVersion": 1,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "durgeshpatel317/multi-client:latest",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "durgeshpatel317/multi-server:latest",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "durgeshpatel317/multi-worker:latest",
            "hostname": "worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "durgeshpatel317/multi-nginx:latest",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "client",
                "server"
            ],
            "memory": 128
        }
    ]
}